


<!-- What posts are allowed, use icon-user, icon-cog, icon-empty-star -->
<% if icon_allowed.real_allowed %>
  <i class="icon-user"></i>
<% end %>
<% if icon_allowed.pseudo_allowed %>
  <i class="icon-cog"></i>
<% end %>
<% if icon_allowed.anon_allowed %>
  <i class="icon-star-empty"></i>
<% end %>
